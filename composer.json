{
    "name": "n1215/lara-adr",
    "description": "Action-Domain-Responder pattern with laravel",
    "keywords": ["adr", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.1.3",
        "ext-json": "*",
        "illuminate/http": ">=5.8",
        "illuminate/routing": ">=5.8",
        "illuminate/support": ">=5.8"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "N1215\\LaraAdr\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "N1215\\LaraAdr\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "N1215\\LaraAdr\\Providers\\LaraAdrServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": [
            "vendor/bin/phpunit"
        ]
    }
}
